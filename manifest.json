{
  "manifest_version": 3,
  "version": "1.2.4",
  "name": "PlaceNoter",
  "description": "Turns browser's home page to a Note-Taking-App.",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ],
  "permissions": [
    "storage",
    "unlimitedStorage"
  ]
}
